@model YuHan.CabsBooking.ApplicationCore.Models.Request.BookingHistoryAddRequestModel

@{
    ViewData["Title"] = "Add BookingHistory";
}

<h1>
    @ViewData["Title"]
</h1>
<div>
    <a asp-controller="BookingHistory" asp-action="Index">Back to History Page</a>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Add">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>


            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BookingDate"></label>
                <input asp-for="BookingDate" class="form-control" />
                <span asp-validation-for="BookingDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BookingTime"></label>
                <input asp-for="BookingTime" class="form-control" />
                <span asp-validation-for="BookingTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FromPlaceId"></label>
                <input asp-for="FromPlaceId" class="form-control" />
                <span asp-validation-for="FromPlaceId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ToPlaceId"></label>
                <input asp-for="ToPlaceId" class="form-control" />
                <span asp-validation-for="ToPlaceId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PickUpAddress"></label>
                <input asp-for="PickUpAddress" class="form-control" />
                <span asp-validation-for="PickUpAddress" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Landmark"></label>
                <input asp-for="Landmark" class="form-control" />
                <span asp-validation-for="Landmark" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PickupDate"></label>
                <input asp-for="PickupDate" class="form-control" />
                <span asp-validation-for="PickupDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PickupTime"></label>
                <input asp-for="PickupTime" class="form-control" />
                <span asp-validation-for="PickupTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CabTypeId"></label>
                <input asp-for="CabTypeId" class="form-control" />
                <span asp-validation-for="CabTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ContactNo"></label>
                <input asp-for="ContactNo" class="form-control" />
                <span asp-validation-for="ContactNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CompTime"></label>
                <input asp-for="CompTime" class="form-control" />
                <span asp-validation-for="CompTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Charge"></label>
                <input asp-for="Charge" class="form-control" />
                <span asp-validation-for="Charge" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Feedback"></label>
                <input asp-for="Feedback" class="form-control" />
                <span asp-validation-for="Feedback" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

